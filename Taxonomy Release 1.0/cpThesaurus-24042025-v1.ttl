        skos:topConceptOf   <http://taxonomie.companypassport.nl> ;
        gs:revisionNo       2 .

taxonomie:legalRepresentative
        rdf:type            skos:Concept ;
        dct:modified        "2025-04-23T13:21:40.259Z"^^xsd:dateTime ;
        skos:altLabel       "Company Representative"@en ;
        skos:definition     "An individual officially representing a legal entity in business or regulatory matters."@en ;
        skos:editorialNote  "The values for Legal Representative can be a Contact Person and/or a UBO Statutory Director."@en ;
        skos:inScheme       <http://taxonomie.companypassport.nl> ;
