        skos:topConceptOf   <http://taxonomie.companypassport.nl> ;
        gs:revisionNo       1 .

taxonomie:percentageOfControl
        rdf:type           skos:Concept ;
        dct:modified       "2025-04-03T14:55:32.245Z"^^xsd:dateTime ;
        skos:altLabel      "Percentage van verkregen zeggenschapsbelang"@nl ;
        skos:definition    "Het percentage zeggenschap dat een UBO heeft over een entiteit."@nl , "The percentage of control that a UBO has over an entity."@en ;
        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "Percentage of acquired control"@en ;
        skos:scopeNote     "Dit percentage geeft aan in hoeverre een UBO beslissingen kan beïnvloeden."@nl ;
        skos:topConceptOf  <http://taxonomie.companypassport.nl> .

taxonomie:UBODeFactoControl
        rdf:type           skos:Concept ;
        skos:altLabel      "Feitelijke UBO"@nl , "UBO Actual Control"@en ;
        skos:broader       taxonomie:UBO , begrippen:natuurlijkPersoon ;
        skos:definition    "A natural person who ultimately owns or controls a legal entity through informal or indirect means, without necessarily holding shares or voting rights."@en , "Een natuurlijk persoon die uiteindelijk zeggenschap heeft over een rechtspersoon via informele of indirecte middelen, zonder noodzakelijkerwijs aandelen of stemrechten te bezitten."@nl ;
        skos:example       "Een externe adviseur zonder formeel eigendom, van wie de beslissingen echter altijd worden overgenomen door het bestuur, kan worden aangemerkt als UBO op basis van feitelijke zeggenschap."@nl , "An external advisor who has no formal ownership, but whose decisions are always followed by the board, may be considered a UBO based on de facto control."@en ;
        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "UBO op basis van feitelijke zeggenschap"@nl , "UBO Based on De Facto Control"@en ;
        skos:scopeNote     "Used when ownership or control is exercised through influence, arrangements, or decision-making power not formally recorded in ownership structures."@en , "Wordt gebruikt wanneer zeggenschap wordt uitgeoefend via invloed, afspraken of beslissingsmacht die niet formeel is vastgelegd in eigendomsstructuren."@nl ;
        skos:topConceptOf  <http://taxonomie.companypassport.nl> .

begrippen:functie  rdf:type  skos:Concept ;
        dct:modified       "2025-04-07T10:21:17.076Z"^^xsd:dateTime ;
        skos:broader       begrippen:functionaris ;
        skos:definition    "Een specifieke rol of positie binnen een organisatie, meestal geassocieerd met bepaalde taken en verantwoordelijkheden."@nl ;
        skos:example       "commissaris, vereffenaar, uitsluitend enig aandeelhouder, bestuurder in one-tier board, uitvoerend bestuurder, niet uitvoerend bestuurder"@nl ;
        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "Functie"@nl , "Function"@en ;
        skos:scopeNote     "GLEIF kent de volgende functiebenamingen:\nAandeelhouder\nBeheerder\nBeheerder / Beherend Vennoot\nBeherend Vennoot\nBestuurder\nBestuurder / bestuurslid\nCommissaris\nCurator\nHouder van certificaten (van aandelen)\nLid\nNiet uitvoerend bestuurder\nOprichter\nPenningmeester\nSecretaris\nStille Vennoot / Commanditaire Vennoot\nUitvoerend bestuurder\nVennoot\nVennoot / Maat\nVereffenaar\nVertegenwoordigingsbevoegde\nVoorzitter\nVoorzitter bestuur\nVoorzitter raad van commissarissen"@en ;
        skos:topConceptOf  <http://taxonomie.companypassport.nl> .

kvk:DA5898460  rdf:type    skos:Concept ;
        dct:modified       "2025-04-03T14:28:49.193Z"^^xsd:dateTime ;
        skos:definition    "Een rechtsvorm is de gekozen juridische vorm waarbinnen de activiteiten van een onderneming or rechtspersoon worden uitgevoerd."@nl ;
        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "Legal form type"@en , "Rechtsvorm"@nl ;
        skos:related       kvk:DA5899751 ;
        skos:scopeNote     "GLEIF kent de volgende waarden:\n\nvereniging met volledige rechtsbevoegdheid\neenmanszaak\nbesloten vennootschap met beperkte aansprakelijkheid\nEuropees Economisch Samenwerkingsverband\nvennootschap onder firma\nvereniging met beperkte rechtsbevoegdheid\nmaatschap\npubliekrechtelijke rechtspersoon\nnaamloze vennootschap\nEuropese naamloze vennootschap (Societas Europaea)\ncommanditaire vennootschap\nonderlinge waarborgmaatschappij\nrechtspersoon in oprichting\nvereniging van eigenaars\nfonds voor gemene rekening\nkerkgenootschap\nOverige Privaatrechtelijke Rechtspersoon\ncoöperatie\nrederij\nstichting\nSocietas Cooperativa Europaea"@nl-nl ;
        skos:topConceptOf  <http://taxonomie.companypassport.nl> .

begrippen:geslachtsnaam
        rdf:type           skos:Concept ;
