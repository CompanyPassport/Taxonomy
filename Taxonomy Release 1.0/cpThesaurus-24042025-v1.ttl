        rdf:type            skos:Concept ;
        dct:modified        "2025-04-23T13:31:33.99Z"^^xsd:dateTime ;
        skos:altLabel       "Amount of Contributed Funds"@en ;
        skos:definition     "The amount of financial resources contributed to an organization from a specific origin."@en ;
        skos:editorialNote  "The calculation of contributed funds may follow financial reporting standards and legal requirements."@en ;
        skos:inScheme       <http://taxonomie.companypassport.nl> ;
        skos:prefLabel      "Herkomst kapitaalbedrag"@nl-nl , "Equity Origin Amount"@en ;
        skos:scopeNote      "This concept is used to quantify the funds injected into an organization, categorized by their source."@en ;
