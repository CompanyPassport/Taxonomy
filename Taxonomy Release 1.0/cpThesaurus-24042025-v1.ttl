begrippen:functionaris
        rdf:type           skos:Concept ;
        dct:modified       "2025-04-23T13:35:13.38Z"^^xsd:dateTime , "2025-04-03T14:31:12.646Z"^^xsd:dateTime ;
        skos:altLabel      "Bevoegde vertegenwoordiger"@nl-nl ;
