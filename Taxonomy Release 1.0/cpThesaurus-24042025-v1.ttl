        skos:definition    "De naam van een openbare ruimte is de door het bevoegde gemeentelijke orgaan aan een openbare ruimte toegekende benaming en een authentiek gegeven."@nl ;
        skos:example       "Kerkstraat"@nl ;
        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "Straatnaam"@nl-nl , "Street name"@en ;
        skos:scopeNote     "Street of the person/legal person."@en ;
        skos:topConceptOf  <http://taxonomie.companypassport.nl> ;
        gs:revisionNo      2 .

taxonomie:emailAddress
        rdf:type            skos:Concept ;
        dct:modified        "2025-04-23T13:28:49.109Z"^^xsd:dateTime , "2025-04-03T14:25:51.457Z"^^xsd:dateTime ;
        skos:altLabel       "E-mail"@nl ;
        skos:definition     "A unique identifier for an electronic mail account used for sending and receiving messages over the Internet."@en ;
        skos:editorialNote  "Ensure email addresses are stored in lowercase to avoid case sensitivity issues."@en ;
        skos:inScheme       <http://taxonomie.companypassport.nl> ;
        skos:prefLabel      "E-mailadres"@nl-nl , "Email Address"@en ;
        skos:related        kvk:DA346652927 ;
