        skos:prefLabel      "Tax Administration"@en ;
        skos:related        kvk:DA5899751 ;
        skos:scopeNote      "This concept refers to institutions that oversee taxation, enforce tax laws, and process tax-related matters."@en ;
        skos:topConceptOf   <http://taxonomie.companypassport.nl> .

kvk:DA5899331  rdf:type    skos:Concept ;
        dct:modified       "2025-04-03T14:26:34.275Z"^^xsd:dateTime ;
        skos:altLabel      "Legal Person Id"@en , "EUID"@en ;
        skos:definition    "A unique identifier assigned to companies within the European Union to facilitate cross-border recognition and information exchange between national business registers."@en ;
        skos:example       "Een Nederlands voorbeeld van de EUID: NL(een code voor het land van inschrijving)NHR(een code voor het register).(een punt als scheidingsteken)67435955(het nationale inschrijfnummer)"@nl ;
        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "European Unique Identifier"@en ;
        skos:related       taxonomie:LPID ;
