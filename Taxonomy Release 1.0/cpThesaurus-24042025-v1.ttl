taxonomie:LPID  rdf:type   skos:Concept ;
        dct:modified       "2025-04-03T14:29:59.54Z"^^xsd:dateTime , "2025-04-23T13:20:06.699Z"^^xsd:dateTime ;
        skos:altLabel      "LPID"@en ;
        skos:broader       kvk:DA5899751 ;
        skos:definition    "A unique identifier assigned to legal entities to facilitate recognition and interaction within digital systems, as defined by Regulation (EU) 2024/2977."@en ;
        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "Legal person identifier"@en , "Identificatienummer rechtspersoon"@nl-nl ;
        skos:related       kvk:DA5899331 , kvk:DA5901425 ;
        skos:scopeNote     "The LPID is used for the secure authentication and identification of legal entities across the European Digital Identity framework. It should not be confused with other business identifiers such as national registration numbers or tax identification numbers."@en ;
        skos:topConceptOf  <http://taxonomie.companypassport.nl> ;
        gs:revisionNo      1 .

taxonomie:telephoneNumber
        rdf:type            skos:Concept ;
