        skos:inScheme      <http://taxonomie.companypassport.nl> ;
        skos:prefLabel     "Postal code"@en , "Postcode"@nl-nl ;
        skos:scopeNote     "Postal code of the person/legal person."@en ;
        skos:topConceptOf  <http://taxonomie.companypassport.nl> ;
        gs:revisionNo      2 .

